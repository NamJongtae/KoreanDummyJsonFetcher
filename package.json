{
  "name": "korean-dummy-json",
  "version": "1.0.0",
  "description": "한국어 더미 데이터 생성 라이브러리",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "unpkg": "dist/index.min.js",
  "jsdelivr": "dist/index.min.js",
  "homepage": "https://koreandummyjson.site",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NamJongtae/KoreanDuumyJsonLibrary.git"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "esbuild": "^0.25.5",
    "tsc-esm-fix": "^3.1.2",
    "typescript": "^5.8.3"
  },
  "scripts": {
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "move:cjs": "powershell -Command \"Move-Item -Path dist/cjs/index.js -Destination dist/index.cjs -Force\"",
    "build:min": "esbuild dist/index.js --minify --outfile=dist/index.min.js",
    "move:esm": "powershell -Command \"Move-Item -Path dist/esm/index.js -Destination dist/index.js -Force\"",
    "fix:index-paths": "node fix-index-paths.js",
    "build": "npm run build:cjs && npm run build:esm && npm run move:cjs && tsc-esm-fix dist && npm run move:esm && npm run build:min && npm run fix:index-paths"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "keywords": [
    "korean",
    "dummy",
    "json",
    "dummy-json",
    "dummy-json-korean",
    "korean-dummy-json",
    "한국어",
    "한글",
    "더미",
    "더미데이터"
  ],
  "author": "JongTae Nam",
  "license": "MIT"
}
